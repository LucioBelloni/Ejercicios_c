#include <stdio.h>
#include <stdlib.h>

int PedirNumero(int numUno, int numDOs);


int main(void) {
	setbuf(stdout, NULL);




	return EXIT_SUCCESS;
}

void PedirNumero(int numUno, int numDOs){

	do{
		printf("Ingrese el numero");
		scanf("%d",&numUno);

	}while(numUno < 10 || numUno > 100);


	do{
		printf("Ingrese el numero");
		scanf("%d",&numDOs);

	}while(numDOs < 10 || numDOs > 100);

}
