#include <stdio.h>
#include <stdlib.h>

int restarUno(int numUno, int numDos);
void pedirNumero(void);

int main(void) {
	setbuf(stdout, NULL);

	int numUno;
	int numDos;
	int resta;

	numUno = pedirNumero();



	return EXIT_SUCCESS;
}

void pedirNumero(void){

	int numUno;


	printf("Ingrese un numero");
		scanf("%d", &numUno);



}

int restarUno(int numUno, int numDos){
	int resultado;

		pedirNumero();


	resultado = numUno - numDos;

	return resultado;

}
