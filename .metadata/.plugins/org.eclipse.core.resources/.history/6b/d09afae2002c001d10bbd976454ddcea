#include <stdio.h>
#include <stdlib.h>

int PedirNumero(void);


int main(void) {




	return EXIT_SUCCESS;
}


int PedirNumero(void){

		int numUno;

		do{
			printf("Ingrese el numero");
			scanf("%d",&numUno);

		}while(numUno < 10 || numUno > 100);

		return numUno;
	}
