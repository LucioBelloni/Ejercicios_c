#include <stdio.h>
#include <stdlib.h>

int restarUno(int numUno, int numDos);

int main(void) {
	setbuf(stdout, NULL);

	int numUno;
	int numDos;
	int resta;

	resta = restarUno(numUno,numDos);
	printf("La resta es %d", resta);

	return EXIT_SUCCESS;
}

int restarUno(int numUno, int numDos){
	int resultado;

	printf("Ingrese un numero");
	scanf("%d", &numUno);

	printf("Ingrese un numero");
	scanf("%d", &numDos);

	resultado = numUno - numDos;

	return resultado;

}
