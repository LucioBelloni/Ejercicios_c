#include <stdio.h>
#include <stdlib.h>

int realizarDescuento();

int main(void) {
	setbuf(stdout, NULL);

	int numero;

	do{
		printf("Ingrese el numero");
		scanf("%d",&numero);

	}while(numero < 10 || numero > 100);


	return EXIT_SUCCESS;
}
