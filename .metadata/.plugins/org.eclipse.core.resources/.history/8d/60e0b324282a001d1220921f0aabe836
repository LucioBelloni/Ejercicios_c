#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	do{
		printf("Ingrese la opcion\n"
			"a -Iniciar \n"
			"b -Procesar \n"
			"c -Finalizar \n"
			"d -Salir\n");



	}while();







	return EXIT_SUCCESS;
}
