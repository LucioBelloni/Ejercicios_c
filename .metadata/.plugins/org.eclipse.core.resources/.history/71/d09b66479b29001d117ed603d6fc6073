#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int numA;
	int numB;
	int numC;
	    printf("Ingrese un numero");
	    scanf("%d", &numA);
	    printf("Ingrese un numero");
	    scanf("%d", &numB);
	    printf("Ingrese un numero");
	    scanf("%d", &numC);
	 if((numA > numB && numA < numC) || (numA > numC && numA < numB)){
		 printf("El numero intermedio es: %d", numA);
	    }
	 else{
		 if((numB > numA && numB < numC) || (numB > numC && numB < numA)){
	    printf("El numero intermedio es %d", numB);
	    }
	    else{
	    	if((numC > numA && numC < numB) || (numC > numB && numC < numA)){
	    			printf("El numero intermedio es: %d", numC);
	    		}else{
	    			printf("No hay numero intermedio");
	    		}
	    	}
	    }

	return EXIT_SUCCESS;
}
