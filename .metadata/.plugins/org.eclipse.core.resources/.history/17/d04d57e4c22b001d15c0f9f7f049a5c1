#include <stdio.h>
#include <stdlib.h>

int PedirNumero(void);
int realiciarOperacion(void);



int main(void) {
	setbuf(stdout, NULL);




	return EXIT_SUCCESS;
}

int PedirNumero(void){

	int numUno;

	do{
		printf("Ingrese el numero");
		scanf("%d",&numUno);

	}while(numUno < 10 || numUno > 100);

	return numUno;
}

int realiciarOperacion(void){

	char sumaResta;

	do{
		printf(" Ingrese  's'-sumar o 'r'-restar,");
		scanf("%c",&sumaResta);

	}while





}
