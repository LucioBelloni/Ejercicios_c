#include <stdio.h>
#include <stdlib.h>

int realizarDescuento(int num);


int main(void) {
	setbuf(stdout, NULL);

	int numero1;

	do{
		printf("Ingrese el numero");
		scanf("%d",&numero1);

	}while(numero1 < 10 || numero1 > 100);


	return EXIT_SUCCESS;
}
